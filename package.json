{
	"name": "web",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"aot": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve --configuration=aot",
		"build": "ng build",
		"build-prod": "ng build --configuration production && node compress.js",
		"build:ssr": "ng build --configuration development && ng run web:server:development",
		"build:ssr:production": "ng build --configuration production && ng run web:server:production",
		"prerender": "ng run web:prerender:development",
		"prerender:production": "ng run web:prerender:production",
		"serve": "ng serve --host localhost --configuration=development",
		"serve:ssr": "ng run web:serve-ssr",
		"build-prender-serve-ssr": "npm run build:ssr && npm run prerender && node dist/web/server/main.js"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^17.3.11",
		"@angular/cdk": "^17.3.10",
		"@angular/common": "^17.3.11",
		"@angular/compiler": "^17.3.11",
		"@angular/core": "^17.3.11",
		"@angular/fire": "17.1.0",
		"@angular/forms": "^17.3.11",
		"@angular/platform-browser": "^17.3.11",
		"@angular/platform-browser-dynamic": "^17.3.11",
		"@angular/platform-server": "^17.3.11",
		"@angular/router": "^17.3.11",
		"@angular/service-worker": "^17.3.11",
		"@angular/ssr": "^17.3.11",
		"@ngx-translate/core": "^15.0.0",
		"@ngx-translate/http-loader": "^8.0.0",
		"core-js": "^3.37.1",
		"express": "^4.18.2",
		"firebase": "^10.7.0",
		"intersection-observer": "^0.12.2",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "^0.14.7"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^17.0.2",
		"@angular-devkit/build-angular": "^17.3.8",
		"@angular/cli": "^17.3.8",
		"@angular/compiler-cli": "^17.3.11",
		"@commitlint/cli": "^17.6.3",
		"@commitlint/config-conventional": "^17.6.3",
		"@types/express": "^4.17.17",
		"@types/jasmine": "~4.3.0",
		"@types/lodash": "^4.14.202",
		"@types/node": "^22.9.3",
		"@webdiscus/pug-loader": "^2.11.0",
		"browser-sync": "^3.0.0",
		"dotenv": "^16.4.5",
		"dotenv-webpack": "^8.1.0",
		"jasmine-core": "~4.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"pug": "^3.0.3",
		"pug-lint": "^2.7.0",
		"pug-lint-config-clock": "^2.0.0",
		"pug-plugin-ng": "^0.0.3",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"ulidx": "^2.2.1",
		"webpack-node-externals": "^3.0.0"
	}
}
